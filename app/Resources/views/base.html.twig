<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Emoovz | Votre inventaire{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/global.css') }}" />
            <link href="https://fonts.googleapis.com/css?family=Alegreya+Sans" rel="stylesheet">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
    <div class="container-fluid">
        {% block menu %}
            {% if moveOut.id is defined %}
            {{ render(controller('AppBundle:Menu:userRoom', {'id':moveOut})) }}
        {% endif %}
        {% endblock %}

        {% block body %}
            <p>Pas de texte</p>
        {% endblock %}
    </div>
        {% block javascripts %}
            <script src="{{ asset('build/app.js') }}"></script>
        {% endblock %}
    </body>
</html>
